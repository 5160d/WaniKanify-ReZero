{
	"name": "wanikanify-rezero",
	"displayName": "WaniKanify ReZero",
	"version": "1.0.0",
	"description": "A tool to reinforce your WaniKani vocabulary.",
	"author": "5160d <5160d-dev@pm.me>",
	"scripts": {
		"dev": "plasmo dev",
		"build": "plasmo build",
		"package": "plasmo package",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:e2e": "playwright test",
		"build:all": "plasmo build --zip --target=chrome-mv3 && plasmo build --zip --target=firefox-mv3"
	},
	"dependencies": {
		"@emotion/cache": "11.11.0",
		"@emotion/react": "11.11.4",
		"@emotion/styled": "11.11.5",
		"@mui/icons-material": "5.15.21",
		"@mui/material": "5.15.21",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"fast-deep-equal": "3.1.3"
	},
	"devDependencies": {
		"@babel/core": "7.26.0",
		"@ianvs/prettier-plugin-sort-imports": "4.4.0",
		"@plasmohq/storage": "1.15.0",
		"@types/chrome": "0.0.280",
		"@types/jest": "29.5.12",
		"@types/node": "22.10.2",
		"@types/react": "18.3.17",
		"@types/react-dom": "18.3.5",
		"@playwright/test": "1.45.1",
		"autoprefixer": "10.4.20",
		"@testing-library/jest-dom": "6.6.3",
		"@testing-library/react": "14.2.1",
		"jest-environment-jsdom": "29.7.0",
		"jest": "29.7.0",
		"ts-jest": "29.2.5",
		"ts-node": "10.9.2",
		"plasmo": "0.90.5",
		"postcss": "8.5.1",
		"prettier": "3.4.2",
		"typescript": "5.7.2",
		"tailwindcss": "3.4.17"
	},
	"manifest": {
		"permissions": [
			"storage",
			"activeTab",
			"alarms"
		],
		"web_accessible_resources": [
			{
				"resources": [
					"assets/popup_background.png"
				],
				"matches": [
					"<all_urls>"
				]
			}
		],
		"host_permissions": [
			"https://*/*"
		],
		"short_name": "WaniKanify",
		"action": {
			"default_title": "WaniKanify ReZero",
			"default_popup": "popup.html"
		},
		"browser_specific_settings": {
			"gecko": {
				"id": "5463b340-00a7-4cd1-b5d4-09503e6c2ac3",
				"strict_min_version": "115.0"
			}
		}
	}
}